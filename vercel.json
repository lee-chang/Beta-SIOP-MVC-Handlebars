{
   "version": 2,
   "builds": [
     {
       "src": "dist/app.js",
       "use": "@vercel/node",
       "config": {
         "env": {
           "NODE_ENV": "production"
         }
       }
     }
   ],
   "routes": [
     {
       "src": "/.*",
       "dest": "/dist/app.js"
     }
   ],
   "env": {
     "NODE_ENV": "production"
   },
   "scripts": {
     "build": "npm run build"
   }
 }